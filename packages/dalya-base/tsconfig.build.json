{
  // This config is for emitting declarations (.d.ts) only
  // Actual .ts source files are transpiled via rollup
  "extends": "./tsconfig",
  "compilerOptions": {
    "sourceMap": false,
    "outDir": "build",
    "rootDir": "./src"
  },
  "include": ["./src/**/*.ts*"],
  "exclude": ["src/**/*.spec.ts*", "src/**/*.test.ts*"]
}
