{
  "name": "dalya",
  "version": "1.0.10",
  "description": "An example component UI library for react",
  "main": "base/index.js",
  "types": "base/index.d.ts",
  "files": [
    "base"
  ],
  "keywords": [
    "react",
    "react-component",
    "dalya"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jiiinCho/dalya.git",
    "directory": "packages/dalya-base"
  },
  "scripts": {
    "build": "rm -rf base && yarn build:base && yarn build:types",
    "build:base": "yarn babel ./src -d ./base --config-file ../../babel.config.js -x .js,.ts,.tsx --env-name=production",
    "build:types": "yarn tsc -b tsconfig.build.json",
    "test": "cd ../../ && yarn jest --testPathPattern=packages/dalya-base",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "author": "jiiinâœ¨",
  "license": "MIT",
  "peerDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@types/react": "^18.0.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "react-is": "^18.2.0"
  }
